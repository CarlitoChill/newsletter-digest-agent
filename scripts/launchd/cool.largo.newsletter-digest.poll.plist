<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>cool.largo.newsletter-digest.poll</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>PROJECT_PATH_PLACEHOLDER/scripts/run_poll.sh</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>6</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
	<key>WorkingDirectory</key>
	<string>PROJECT_PATH_PLACEHOLDER</string>
	<key>StandardOutPath</key>
	<string>PROJECT_PATH_PLACEHOLDER/data/poll_stdout.log</string>
	<key>StandardErrorPath</key>
	<string>PROJECT_PATH_PLACEHOLDER/data/poll_stderr.log</string>
</dict>
</plist>
